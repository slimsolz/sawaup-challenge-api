// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Course {
  id Int @default(autoincrement()) @id
  name String
  url String @unique
  skills Skill[]
}

model Skill {
  id Int @default(autoincrement()) @id
  name String
  skill Course @relation(fields: [skillId], references: [id])
  skillId Int
}
